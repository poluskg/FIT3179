{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "https://poluskg.github.io/FIT3179/Assignment_2/data/VoterTurnout.csv"
  },
  "mark": {
    "type": "line",
    "point": true
  },
  "encoding": {
    "x": {"field": "Year"},
    "y": {"field": "Value", "type": "quantitative", "title": null},
    "color": {
      "field": "Category",
      "title": null,
      "scale": {"range": ["purple", "teal"]}
    },
    "tooltip": [
      {"field": "Year"},
      {"field": "Category"},
      {"field": "Value", "title": "% of Eligible Voters", "format":".1%"}
    ]
  },
  "width": 400,
  "height": 300
}
